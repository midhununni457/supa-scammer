
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
model Url {
  id         Int     @id @default(autoincrement())
  url        String  @unique
  errorCount Int     @default(0)
  createdAt  DateTime @default(now())
}